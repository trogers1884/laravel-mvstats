{
  "name": "trogers1884/laravel-mvstats",
  "description": "Laravel package for PostgreSQL statistics",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Rogers"
    },
    {
      "name": "Jeremy Gleed"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Trogers1884\\LaravelMvstats\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trogers1884\\LaravelMvstats\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trogers1884\\LaravelMvstats\\LaravelMvstatsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}